{
  "name": "cooni-backend",
  "version": "0.0.1",
  "description": "Cooni Backend Server",
  "main": "index.js",
  "scripts": {
    "update:packages": "node wipe-dependencies.js && rm -rf node_modules && npm update --save-dev && npm update --save",
    "postinstall": "tsc",
    "lint": "tslint -c tslint.json 'server/**/*.ts'",
    "tsc": "tsc",
    "watch": "tsc -w",
    "start": "tsc -w & nodemon dist/index.js",
    "start:win": "concurrently --kill-others \"tsc -w\" \"nodemon dist/index.js\"",
    "test": "mocha --reporter spec --compilers ts:ts-node/register 'tests/**/*.test.ts'",
    "test:prod": "NODE_ENV=production mocha --reporter spec --compilers ts:ts-node/register 'tests/**/*.test.ts'",
    "test:local": "tsc && node_modules/.bin/mocha dist/test/*.spec.js",
    "dev": "NODE_ENV=development pm2 start dist/index.js",
    "production": "NODE_ENV=production pm2 start dist/index.js"
  },
  "author": "hyochan",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "biguint-format": "^1.0.0",
    "body-parser": "^1.17.1",
    "composable-middleware": "^0.3.0",
    "connect-redis": "^3.2.0",
    "cookie-parser": "^1.4.3",
    "crypto-random-string": "^1.0.0",
    "del": "^3.0.0",
    "ejs": "^2.5.6",
    "express": "^4.15.2",
    "express-session": "^1.15.1",
    "flake-idgen": "^1.1.0",
    "http": "0.0.0",
    "jsonwebtoken": "^8.1.0",
    "localize": "^0.4.7",
    "lodash": "^4.17.4",
    "mobile-detect": "^1.3.5",
    "morgan": "^1.8.1",
    "multer": "^1.3.0",
    "mysql2": "^1.4.2",
    "nodemailer": "^3.1.8",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "sequelize": "^4.13.8",
    "sha256": "^0.2.0",
    "typescript": "^2.2.1",
    "util": "^0.10.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.0",
    "@types/chai": "^4.0.10",
    "@types/chai-http": "^3.0.4",
    "@types/cookie-parser": "^1.3.30",
    "@types/express": "^4.11.1",
    "@types/express-session": "1.15.8",
    "@types/jsonwebtoken": "^7.2.6",
    "@types/lodash": "^4.14.105",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.7",
    "@types/passport": "^0.4.3",
    "@types/passport-local": "^1.0.29",
    "@types/sequelize": "^4.27.10",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "mocha": "^3.5.3",
    "mysql": "^2.13.0",
    "should": "^13.1.2",
    "source-map-support": "^0.5.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.7.0",
    "tslint-no-unused-expression-chai": "0.0.3",
    "webpack": "^3.7.1"
  }
}
